{
    "compilerOptions": {        
        "baseUrl": ".",
        "outDir": "dist",
        "declaration": true,
        "sourceMap": false,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "allowJs": false,
        "strict": false,
        "noUnusedLocals": false,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "rootDir": ".",
        "types": ["vite/client"]
    },
    "include": ["src"]
}